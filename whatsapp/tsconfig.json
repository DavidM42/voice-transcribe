{
	"module": "true", 
	"compilerOptions": {
		"rootDirs": ["./", "../src/logic/"],
		"sourceRoot": "./",
		"outDir": "./dist",
		"moduleResolution": "node",
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "es2021",
		"module": "es6"
	},
	"include": [
		"./",
		"../src/logic/"
	],
	"exclude": [
		"./dist/"
	]
}
