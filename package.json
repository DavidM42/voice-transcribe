{
	"name": "voice-transcribe",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"host": "vite build && vite preview --host",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"openapi-whisper": "openapi-ts",
		"whatsapp-bot": "npx tsx src/external-services/whatsapp/cli-bot.ts",
		"hybrid-server": "npm run build && tsx -r dotenv/config src/bot-custom-server-hybrid.ts"
	},
	"devDependencies": {
		"@hey-api/openapi-ts": "^0.43.0",
		"@skeletonlabs/skeleton": "^0.124.2",
		"@sveltejs/adapter-node": "^5.0.1",
		"@sveltejs/kit": "^1.5.0",
		"@types/express": "^4.17.21",
		"@types/qrcode": "^1.5.5",
		"@types/qrcode-terminal": "^0.12.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.13",
		"dotenv": "^16.4.5",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"express": "^4.19.2",
		"express-basic-auth": "^1.2.1",
		"postcss": "^8.4.21",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"qrcode": "^1.5.3",
		"qrcode-terminal": "^0.12.0",
		"sass": "^1.58.3",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.4.1",
		"tsx": "^4.9.0",
		"typescript": "^5.0.0",
		"vite": "^4.0.0",
		"whatsapp-web.js": "pedroslopez/whatsapp-web.js#webpack-exodus"
	},
	"type": "module"
}
